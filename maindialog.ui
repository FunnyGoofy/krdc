<connections>
    <connection>
        <sender>connectButton</sender>
        <signal>clicked()</signal>
        <receiver>MainDialog</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>cancelButton</sender>
        <signal>clicked()</signal>
        <receiver>MainDialog</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>serverInput</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>MainDialog</receiver>
        <slot>hostChanged(const QString&amp;)</slot>
    </connection>
    <connection>
        <sender>browseButton</sender>
        <signal>toggled(bool)</signal>
        <receiver>browsingView</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>browseButton</sender>
        <signal>clicked()</signal>
        <receiver>MainDialog</receiver>
        <slot>toggleBrowsingArea()</slot>
    </connection>
    <connection>
        <sender>scopeCombo</sender>
        <signal>activated(int)</signal>
        <receiver>MainDialog</receiver>
        <slot>rescan()</slot>
    </connection>
    <connection>
        <sender>rescanButton</sender>
        <signal>clicked()</signal>
        <receiver>MainDialog</receiver>
        <slot>rescan()</slot>
    </connection>
    <connection>
        <sender>preferencesButton</sender>
        <signal>clicked()</signal>
        <receiver>MainDialog</receiver>
        <slot>preferencesClicked()</slot>
    </connection>
</connections>
